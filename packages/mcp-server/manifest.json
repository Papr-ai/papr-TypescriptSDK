{
  "dxt_version": "0.2",
  "name": "@papr/memory-mcp",
  "version": "1.19.0",
  "description": "The official MCP Server for the Papr API",
  "author": {
    "name": "Papr",
    "email": "support@papr.ai"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Papr-ai/papr-TypescriptSDK.git"
  },
  "homepage": "https://github.com/Papr-ai/papr-TypescriptSDK/tree/main/packages/mcp-server#readme",
  "documentation": "https://platform.papr.ai",
  "server": {
    "type": "node",
    "entry_point": "index.js",
    "mcp_config": {
      "command": "node",
      "args": ["${__dirname}/index.js"],
      "env": {
        "PAPR_MEMORY_API_KEY": "${user_config.PAPR_MEMORY_API_KEY}",
        "PAPR_MEMORY_Session_Token": "${user_config.PAPR_MEMORY_Session_Token}",
        "PAPR_MEMORY_BEARER_TOKEN": "${user_config.PAPR_MEMORY_BEARER_TOKEN}"
      }
    }
  },
  "user_config": {
    "PAPR_MEMORY_API_KEY": {
      "title": "X-API-Key",
      "description": "",
      "required": true,
      "type": "string"
    },
    "PAPR_MEMORY_Session_Token": {
      "title": "X-Session-Token",
      "description": "",
      "required": false,
      "type": "string"
    },
    "PAPR_MEMORY_BEARER_TOKEN": {
      "title": "bearer_token",
      "description": "",
      "required": false,
      "type": "string"
    }
  },
  "tools": [],
  "tools_generated": true,
  "compatibility": {
    "runtimes": {
      "node": ">=18.0.0"
    }
  },
  "keywords": ["api"]
}
